library ieee;
use ieee.std_logic_1164.all;

entity OF_check is
  port ( a: in std_logic;
         b: in std_logic;
         sum: in std_logic;
         ofout: out std_logic
       );
     end OF_check;
     
     architecture struct of OF_check is
       
       component NOT_gate is
         port ( a: in std_logic;
                b: out std_logic
              );
            end component;
            
        component AND_gate is
          port ( x: in std_logic;
                 y: in std_logic;
                 z: out std_logic
               );
             end component;
             
        component XOR_gate is
          port ( a: in std_logic;
                 b: in std_logic;
                 c: out std_logic
               );
             end component;
             
             